<h1>Quesadillas</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quesadillas.each do |quesadilla| %>
      <tr>
        <td><%= quesadilla.title %></td>
        <td><%= quesadilla.description %></td>
        <td><%= quesadilla.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>