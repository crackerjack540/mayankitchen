      <div><%= image_tag 'taco.jpg', class: 'img-fluid' %></div>
      <div><%= image_tag 'chimichanga.jpg', class: 'img-fluid' %></div>
      <div><%= image_tag 'seafood.jpg', class: 'img-fluid' %></div>
      <div><%= image_tag 'chicken.jpg', class: 'img-fluid' %></div>
      <div><%= image_tag 'dessert.jpg', class: 'img-fluid' %></div>
    </div>





    <script type="text/javascript">
