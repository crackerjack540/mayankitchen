<div class="row">
  <div class="col-sm-6">
<head>
<% content_for :head do %>
  <meta name="turbolinks-cache-control" content="no-cache">
<% end %>

  <meta name="viewport" content="width=device-width, initial-scale=1">

</head>
<body>


    <div class="carousel">
      <div><%= image_tag 'carousel01.jpg', class: 'img-fluid' %></div>
      <div><%= image_tag 'carousel02.jpg', class: 'img-fluid' %></div>
      <div><%= image_tag 'carousel03.jpg', class: 'img-fluid' %></div>
      <div><%= image_tag 'carousel04.jpg', class: 'img-fluid' %></div>
      <div><%= image_tag 'carousel05.jpg', class: 'img-fluid' %></div>
      <div><%= image_tag 'carousel06.jpg', class: 'img-fluid' %></div>
      <div><%= image_tag 'carousel07.jpg', class: 'img-fluid' %></div>
    </div>


    <script type="text/javascript">
      $(document).ready(function(){
        $('.carousel').slick({
            infinite: true,
            autoplay: true,
            autoplaySpeed: 5000,
            arrows: false,
            fade: true,
            slidesToShow: 1,
        });
      });
    </script>

</div>

<div class="col-sm-6">
