<p id="notice"><%= notice %></p>

<h1>Antojiotos (Appetizers)</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @appetizers.each do |appetizer| %>
      <tr>
        <td><%= appetizer.title %></td>
        <td><%= appetizer.description %></td>
        <td><%= appetizer.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>
