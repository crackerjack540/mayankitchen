<h1>Enchiladas</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enchiladas.each do |enchilada| %>
      <tr>
        <td><%= enchilada.title %></td>
        <td><%= enchilada.description %></td>
        <td><%= enchilada.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>