<h1>Tacos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tacos.each do |taco| %>
      <tr>
        <td><%= taco.title %></td>
        <td><%= taco.description %></td>
        <td><%= taco.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>