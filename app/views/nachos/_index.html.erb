
<h1>Nachos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nachos.each do |nacho| %>
      <tr>
        <td><%= nacho.title %></td>
        <td><%= nacho.description %></td>
        <td><%= nacho.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>

