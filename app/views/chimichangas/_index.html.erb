<h1>Chimichangas</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chimichangas.each do |chimichanga| %>
      <tr>
        <td><%= chimichanga.title %></td>
        <td><%= chimichanga.description %></td>
        <td><%= chimichanga.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>